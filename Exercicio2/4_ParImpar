import java.util.Scanner;

public class FaturamentoVendas {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        System.out.print("Digite o numero: ");
        int num = sc.nextInt();
        if (num % 2 == 0){
            
             System.out.print("Par");
            }
        else {
            System.out.print("Impar");
        }
        }
     }
